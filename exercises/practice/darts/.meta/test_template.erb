require 'minitest/autorun'
require_relative 'darts'

class DartsTest < Minitest::Test
<% json["cases"].each do |cases| %>
    def test_<%= underscore(cases["description"]) %>
        <%= skip? %> 
        darts = Darts.new(<%= cases["input"]["x"] %>, <%= cases["input"]["y"] %>)
        assert_equal(<%= cases["expected"] %>, darts.score)
    end
    <% end %>
end