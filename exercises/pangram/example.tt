#!/usr/bin/env ruby
# encoding: utf-8
gem 'minitest', '>= 5.0.0'
require 'minitest/autorun'
require_relative 'pangram'

# Test data version:
# <%= sha1 %>
class PangramTest < Minitest::Test<% test_cases.each do |test_case| %>
  def <%= test_case.test_name %>
